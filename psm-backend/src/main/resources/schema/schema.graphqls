type Query{
    FrontCover:[String!]
    login(phoneNumber: String, password: String):Account!
    fasterLogin:Account!
}

type Mutation{
    register(phoneNumber: String, password: String):Account!
}

type Account{
    ID: String!
    status: String!
    phoneNumber: String!
    userName: String!
    profile: String!
    token: String!
}

input AccountInput{
    phoneNumber: String!
    password: String!
}