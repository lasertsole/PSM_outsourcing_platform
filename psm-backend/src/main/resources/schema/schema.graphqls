type Query{
    FrontCover:[String!]
    events:[String!]
    testArr:[String!]
}

type Mutation{
    createEvent(name: String):String
    register(phoneNumber: String, password: String):Account!
}

type Account{
    ID: String!
    status: String!
    phoneNumber: String!
    userName: String!
    profile: String!
    token: String!
}

input AccountInput{
    phoneNumber: String!
    password: String!
}